@model SWMGEGCSS.Models.GestionarEmpresaViewModel
@{
    ViewBag.Title = "_Eliminar_Empresa";
    Layout = "~/Views/Shared/_LayoutEmpresa.cshtml";
    
}

<h2>_Eliminar_Empresa</h2>

<div>
    <div>
        <div class="container form mt-5">


            <div>
                <label>
                    Id
                </label>
               
                    <input type="text" value="" id="emp_telefono" disabled />
              
            </div>
            <div>
                <label>
                    Ruc
                </label>
                <input type="text" value="" id="emp_ruc" disabled />
            </div>
            <div>
                <label>
