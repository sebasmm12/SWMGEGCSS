
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>_LayoutCliente</title>
    <link rel="stylesheet" href="~/Content/fontawesome-all.min.css" />
    <link rel="stylesheet" href="~/Content/animate/animate.css" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Layout_Empresa/alertaservicio.css" />
    <link rel="stylesheet" href="~/Content/animate/animate.css" />
    <link rel="stylesheet" href="~/Content/StyleTP/styleClient.css" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-expand-sm navbar-expand-md navbar-light">
        <a class="navbar-brand" href="#">
            <img src="~/images/GEO-LOGO-FINAL-IDCG.png" class="img-fluid rounded" width="120" />
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarEmpresa" aria-controls="navbarEmpresa" aria-expanded="false" aria-label="Navegacion de la Empresa">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarEmpresa">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-play"></i>
                        Inicio
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-users"></i>
                        Quienes Somos
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-paper-plane"></i>
                        Servicios que Brindamos
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-user-tie"></i>
                        Clientes
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-link"></i>
                        Enlaces de Interes
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-gavel"></i>
                        Normatividad
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")">
                        <i class="fa fa-directions"></i>
                        Contactenos
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link link-inicio" href="@Url.Action("Inicio","Cliente")" data-toggle="modal" data-target="#modalinicio">
                        <i class="fa fa-sign-in-alt"></i>
                        Iniciar Sesión
                    </a>
                </li>
            </ul>
        </div>
    </nav>

    <div>
        @RenderBody()
    </div>
    <div class="modal fade" id="modalinicio" tabindex="-1" role="dialog" aria-labelledby="titulomodal" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content" style="padding-bottom:30px;">
                <div class="modal-header">
                    <h5 class="modal-title titulo-login" id="titulomodal">
                    Geo Consult Service S.A.C
                    </h5>
                </div>
                <div class="modal-body text-center">
                    <div class="row mt-3">
                        <div class="col-sm-12 col-md-12 col-12">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="user">
                                        <i class="fa fa-user"></i>
                                    </span>
                                </div>
                                <input type="text" class="form-control" placeholder="Ingrese nombre de usuario" name="username" id="username" aria-describedby="user" />
                            </div>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-sm-12 col-md-12 col-12">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="pass">
                                        <i class="fa fa-lock"></i>
                                    </span>
                                </div>
                                <input type="password" class="form-control" placeholder="Ingrese Contraseña" name="password" id="password" aria-describedby="pass" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center pl-3">
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-12">
                            <a href="@Url.Action("Registrarse")" class="btn btn-inicio">
                                <i class="fa fa-check">
                                </i>
                                Iniciar Sesion
                            </a>
                        </div>
                        <div class="col-sm-4 col-md-4 col-12">
                            <a href="@Url.Action("Registrarse")" class="btn btn-success">
                                <i class="fa fa-user-alt">
                                </i>
                                Registrarse
                            </a>
                        </div>
                        <div class="col-sm-4 col-md-4 col-12">
                            <button type="button" class="btn btn-danger" data-dismiss="modal">
                            <i class="fa fa-times"></i>
                            Salir</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/fontawesome/fontawesome.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/sweetalert/sweetalert.js"></script>
    <script src="~/Scripts/scrollmagic/scrollmagic.js"></script>
    <script src="~/Scripts/script-autocomplete/jquery-ui-min.js"></script>
    <script src="~/Scripts/Scripts_TP/scriptsCliente.js"></script>
    <script src="https://ajax.cloudflare.com/cdn-cgi/scripts/a2bd7673/cloudflare-static/rocket-loader.min.js" data-cf-settings="c326a13879583a964fd0aec1-|49" defer=""></script>
    @RenderSection("scripts", required: false)
</body>
</html>
